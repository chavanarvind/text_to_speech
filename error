# Path to your Parquet file
file_path = './data/test1.parquet'

# Read the file
df = pd.read_parquet(file_path)

import pandas as pd
pd.set_option('display.max_columns', None)

mask = (
            df["CMPNT_MATL_NUM"].notna() &
            (df["AI_FINAL_SUBCATEGORY"].str.upper() != "OTHER") &
            df["AI_MATCHING_REASON_FINAL_SUBCATEGORY"].isna()
        )
matched_count = mask.sum()
print(f"üîç  {matched_count} rows matched for update.")
# View the first few rows
print(df.head(0))

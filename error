df.to_parquet(output_path, index=False)
datastore.upload_files(
    files=[output_path],
    target_path="mapped_data/final_output/",
    overwrite=True
)

# Write to the final_output folder
output_file = os.path.join(args.final_output, "final_output.parquet")
df.to_parquet(output_file, index=False)
print(f"[INFO] Saved mapped output to: {output_file}")


key_df.to_parquet(key_path, index=False)
datastore.upload_files([...])

key_file = os.path.join(args.key_output, "key_reference.parquet")
key_df.to_parquet(key_file, index=False)
print(f"[INFO] Saved key reference file to: {key_file}")

if 'LOGL_KEY_COMB_COL_VAL' in df.columns:
    total_rows = len(df)
    non_null_count = df['LOGL_KEY_COMB_COL_VAL'].notnull().sum()
    all_null = non_null_count == 0

    if all_null:
        print(f"[üö´ NULL] {basename} ‚û§ All {total_rows} values in LOGL_KEY_COMB_COL_VAL are NULL.")
    else:
        print(f"[‚úÖ] {basename} ‚û§ Non-null LOGL_KEY_COMB_COL_VAL count: {non_null_count} / {total_rows}")
else:
    print(f"[‚ö†Ô∏è MISSING] {basename} ‚û§ Column LOGL_KEY_COMB_COL_VAL is missing entirely!")

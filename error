rejected_idx = collected.key_prediction.isin(known_rejections)

if '668080_PC-0000253' in collected['key_prediction'].values:
    print("[DEBUG] Pair 668080 + PC-0000253 found in matches BEFORE rejection check.")
    if '668080_PC-0000253' in known_rejections:
        print("[DEBUG] Pair 668080 + PC-0000253 IS in rejection file. Will be rejected.")
    else:
        print("[DEBUG] Pair 668080 + PC-0000253 NOT in rejection file. Will stay matched.")

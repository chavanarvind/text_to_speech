import polars as pl

# Path to your Parquet file
file_path = './data/parquet_files/sample_file.parquet'  # Replace with actual file

# Read the file
df = pl.read_parquet(file_path)

# Get unique values in 'Final Category' column
unique_values = df.select(pl.col('Final Category')).unique()

# Print results
print("Unique values in 'Final Category':")
print(unique_values)

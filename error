import sqlite3

def insert_new_keys(db_path, keys):
    conn = sqlite3.connect(db_path, timeout=30)
    cursor = conn.cursor()
    cursor.execute("CREATE TABLE IF NOT EXISTS keys (key TEXT PRIMARY KEY)")
    new_keys = []
    for k in keys:
        try:
            cursor.execute("INSERT INTO keys (key) VALUES (?)", (k,))
            new_keys.append(k)
        except sqlite3.IntegrityError:
            pass  # Already exists
    conn.commit()
    conn.close()
    return new_keys

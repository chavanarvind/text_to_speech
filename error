SELECT DISTINCT 
  SRC_SYS_CD, 
  PLNT_CD, 
  MATL_NUM, 
  CMPNT_MATL_NUM 
FROM hive_metastore.bom_rstr.nlp_input_bom_hier 
WHERE CMPNT_MATL_NUM = '30050912'

SELECT DISTINCT 
  ERP_PART_NUM, 
  CHILD_NM, 
  FG_NM, 
  FG_SPEC_NM, 
  CHILD_TITLE_DESC 
FROM hive_metastore.bom_rstr.nlp_input_p360_bom 
WHERE CHILD_NM = 'PC-0001019' AND ERP_PART_NUM LIKE '%688727250'

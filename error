if not forced_matches.empty:
    matches = pd.concat([matches, forced_matches], ignore_index=True)

matches = (
    matches
    .sort_values(by=["SRC_SYS_CD", "PLNT_CD"], ascending=[False, False])
    .drop_duplicates(subset=["CMPNT_MATL_NUM", "CHILD_NM", "MATL_NUM"], keep="first")
    .reset_index(drop=True)
)

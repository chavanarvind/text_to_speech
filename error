cmp_check_list = ['02109345', '02109344', '02109429', '02150019', '02111769', '02112146', '02111779']
df_check = erp_im[erp_im["CMPNT_MATL_NUM"].isin(cmp_check_list)]

user_logs_path = os.path.join(os.getcwd(), "user_logs")
os.makedirs(user_logs_path, exist_ok=True)
df_check.to_csv(os.path.join(user_logs_path, "erp_im_tracked_ids.csv"), index=False)

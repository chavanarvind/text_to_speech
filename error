-- Check if tracked IDs are missing ERP_PART_DESC
SELECT CMPNT_MATL_NUM AS ERP_PART_ID, CMPNT_MATL_DESC AS ERP_PART_DESC
FROM bom_rstr.nlp_input_bom_hier_dtls
WHERE CMPNT_MATL_NUM IN ('02109345','02109344','02109429','02150019','02111769','02112146','02111779')
  AND (CMPNT_MATL_DESC IS NULL OR TRIM(CMPNT_MATL_DESC) = '')

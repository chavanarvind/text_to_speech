import os

# Define the folder path and the target file name
folder_path = '/path/to/your/folder'
target_file = 'your_target_file.parquet'

# List all files in the folder
file_list = os.listdir(folder_path)

# Find the position of the target file
if target_file in file_list:
    position = file_list.index(target_file) + 1  # Position is 1-based
    print(f"The file '{target_file}' is at position {position}.")
else:
    print(f"The file '{target_file}' was not found in the folder.")

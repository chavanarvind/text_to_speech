df_key = df[key_cols].copy()
df_key = df_key[df_key['LOGL_KEY_COMB_COL_VAL'].notna()].drop_duplicates()


df_key = df[key_cols].copy()
df_key = df_key[df_key['LOGL_KEY_COMB_COL_VAL'].notna()]
df_key = df_key[~df_key['LOGL_KEY_COMB_COL_VAL'].isin(seen_keys)]
df_key = df_key.drop_duplicates()

# Update seen_keys
seen_keys.update(df_key['LOGL_KEY_COMB_COL_VAL'].dropna().unique())

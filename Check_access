erp = erp.dropna(subset=['key_erp', 'PLNT_CD', 'SRC_SYS_CD'])
print("="*60)
print("üîç DEBUG: Checking ERP_PART_ID before dropna")
erp['ERP_PART_ID'] = erp['ERP_PART_ID'].astype(str).str.strip()
erp_id_to_check = "000000000000528926"
erp_id_unpadded = "528926"

debug_row = erp[erp['ERP_PART_ID'].isin([erp_id_to_check, erp_id_unpadded])]
if debug_row.empty:
    print(f"‚ùå ERP_PART_ID {erp_id_to_check} or {erp_id_unpadded} NOT found in ERP before dropna")
else:
    print(f"‚úÖ ERP_PART_ID {erp_id_to_check} or {erp_id_unpadded} FOUND in ERP before dropna")
    print(debug_row[['ERP_PART_ID', 'key_erp', 'PLNT_CD', 'SRC_SYS_CD']])
print("="*60)

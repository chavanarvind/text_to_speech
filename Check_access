SELECT 
    CASE 
        WHEN COUNT(*) > 0 THEN 'YES'
        ELSE 'NO'
    END AS MATCH_EXISTS
FROM 
    hive_metastore.bom_rstr.nlp_input_bom_hier_dtls bhd
JOIN 
    hive_metastore.bom_rstr.nlp_input_p360_bom p360
ON 
    bhd.NEW_MATCH_MATL_NUM = p360.ERP_PART_NUM
WHERE 
    bhd.CMPNT_MATL_NUM = '000000000006139668'
    AND p360.CHILD_NM = 'PC-0000457';


return current_full_matches, unmatched_alternate
user_logs_path = os.path.join(os.getcwd(), "user_logs")
os.makedirs(user_logs_path, exist_ok=True)

# 📝 Save ALL matches to CSV
all_matches_file = os.path.join(user_logs_path, "all_matches_before_save.csv")
current_full_matches.to_csv(all_matches_file, index=False)

print(f"📁 Saved ALL matches to: {all_matches_file}")
